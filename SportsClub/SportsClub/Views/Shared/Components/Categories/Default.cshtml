@model IEnumerable<Category>

<a class="nav-link text-dark dropdown-item" asp-area="" asp-controller="Services" asp-action="Services" asp-route-categoryName="">Усі послуги</a>
<div class="dropdown-divider"></div>
@foreach (var category in Model)
{
	<a class="nav-link text-dark dropdown-item" asp-area="" asp-controller="Services" asp-action="Services" asp-route-categoryName="@category.URLFriendlyName">@category.Name</a>
}